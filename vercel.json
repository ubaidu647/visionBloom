{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/user/(.*)",
      "dest": "src/routes/user/user.js"
    },
    {
      "src": "/api/staff/(.*)",
      "dest": "src/routes/staff/staff.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
